{
  "name": "@changelogify/sdk",
  "version": "0.1.0",
  "description": "Changelogify SDK to embed widgets",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "typecheck": "tsc --noEmit",
    "build": "microbundle -f es,cjs,umd",
    "dev": "microbundle watch -f es,cjs,umd",
    "clean": "rimraf dist"
  },
  "source": "src/index.ts",
  "exports": {
    "types": "./index.d.ts",
    "module": "./dist/changelogify.mjs",
    "import": "./dist/changelogify.mjs",
    "require": "./dist/changelogify.js",
    "default": "./dist/changelogify.mjs"
  },
  "types": "./dist/changelogify.d.ts",
  "main": "./dist/changelogify.js",
  "module": "./dist/changelogify.mjs",
  "unpkg": "./dist/changelogify.umd.js",
  "repository": "Changelogify/sdk-js",
  "keywords": [
    "changelogify",
    "changelogs",
    "sdk",
    "widget"
  ],
  "files": [
    "dist"
  ],
  "homepage": "https://changelogify.news",
  "authors": [
    "derpierre65 <git@derpierre65.dev>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@derpierre65/eslint-config": "github:derpierre65/eslint-config",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "microbundle": "^0.15.1",
    "mitt": "^3.0.1"
  }
}
